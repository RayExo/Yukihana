{
  "name": "yukihana-discord-bot",
  "version": "2.0.0",
  "description": "A versatile Discord.js bot with advanced sharding and modular command architecture",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/shard.js",
    "dev": "node --watch src/shard.js",
    "test": "node --test",
    "setup": "cp .env.example .env && echo 'Created .env file. Please edit it with yaour configuration.'"
  },
  "imports": {
    "#commands/*": "./src/commands/*.js",
    "#structures/*": "./src/structures/*.js",
    "#database/*": "./src/database/*.js",
    "#config/*": "./src/config/*.js",
    "#utils/*": "./src/utils/*.js",
    "#events/*": "./src/events/*.js",
    "#managers/*": "./src/managers/*.js",
    "#handlers/*": "./src/structures/handlers/*.js",
    "#classes/*": "./src/structures/classes/*.js",
    "#db/*": "./src/database/repo/*.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "sharding",
    "modular",
    "yukihana"
  ],
  "author": "Bre4d777",
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.74",
    "axios": "^1.11.0",
    "better-sqlite3": "^11.9.1",
    "chalk": "^5.4.1",
    "discord-hybrid-sharding": "^2.2.6",
    "discord.js": "^14.22.0",
    "discord.js-docs": "^0.4.3",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "lavalink-client": "^2.5.7",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.3",
    "node-fetch": "^2.7.0",
    "undici": "^7.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "eslint-formatter-pretty": "^6.0.1"
  }
}
